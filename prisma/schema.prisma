generator client {
  provider = "prisma-client-js"
}

generator json_server {
  provider       = "prisma-json-server-generator"
  outputFileName = "db_diana.json"
}

datasource db {
 // provider = "sqlite"
 // url      = "file:./dev.db"
 provider = "mysql"
 url = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  userUUID String
  name     String
  username String
}
